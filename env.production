# 生产环境配置
NODE_ENV=production
PORT=3001

# 数据库配置
DATABASE_URL="mysql://username:password@localhost:3306/config_service"

# CORS配置
CORS_ORIGIN=https://mob.greenpencil.top

# API配置
API_PREFIX=/api/v1

# 日志配置
LOG_LEVEL=info

# Redis配置（可选）
REDIS_URL=redis://localhost:6379

# 安全配置
HELMET_ENABLED=true
RATE_LIMIT_ENABLED=true
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# 性能配置
COMPRESSION_ENABLED=true
MORGAN_ENABLED=true 